{
  "name": "Open spectrophotometer",
  "description": "Specrophotometer controller of a open-source arduino based device.",
  "version": "0.0.0.9",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"],
      "transient": true
    }
  },
  "icons": { "16": "icon_16.png", "128": "icon_128.png" },
  "permissions": [
    "webview",
    "serial",
    "storage"
  ],
  "sandbox": {
    "pages": [
      "wrap.html"
    ]
  }
}



