{
  "name": "Cheminfo demo",
  "description": "Examples of communication between the Visualizer and Chrome API",
  "version": "0.0.2",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"],
      "transient": true
    }
  },
  "icons": { "16": "icon_16.png", "128": "icon_128.png" },
  "permissions": [
    "webview",
    "storage",
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ]
}
